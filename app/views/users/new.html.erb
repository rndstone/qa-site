<%= form_for(@users) do |f|%>
  <%= f.label :name, 'name'%>
  <%= f.text_field :name %>
  
  <%= f.label :email, 'email' %>
  <%= f.email_field :email %>
  
  <%= f.label :password, 'password' %>
  <%= f.password_field :password %>
  
  <%= f.label :password_confirmation, 'Confirmation' %>
  <%= f.password_field :password_confirmation, class: 'form-control' %>
 
  <%= f.submit 'Sign up' %>
<% end %>